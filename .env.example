# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5005
REACT_APP_API_URL=http://localhost:5005/api

# Payment Gateway
REACT_APP_RAZORPAY_KEY_ID=your_razorpay_key_id

# Social Media Integration
REACT_APP_INSTAGRAM_APP_ID=your_instagram_app_id
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=your_google_analytics_id

# Environment
REACT_APP_NODE_ENV=development
REACT_APP_FRONTEND_URL=http://localhost:3000

# File Upload
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=image/*,application/pdf,application/zip

# Features
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_INSTAGRAM_AUTOMATION=true
REACT_APP_ENABLE_EMAIL_CAMPAIGNS=true
REACT_APP_ENABLE_WHATSAPP_CAMPAIGNS=true

# UI Configuration
REACT_APP_THEME=light
REACT_APP_BRAND_NAME=Superlinks
REACT_APP_SUPPORT_EMAIL=support@superlinks.ai